{
  "source": {
    "include": [
      "./src/js/"
    ],
    "includePattern": "\\.(js|jsx)$",
    "exclude": [
      "node_modules/"
    ]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}